	public static final int LEFT = 4;
	public static final int RIGHT = 4;
	public static final int TOP = 4;
	public static final int BOTTOM = 6;
	public static final int COMPONENT_WIDTH = 100;
	public static final int FIELD_WIDTH = 100;
	public static final int COMPONENT_HEIGHT = 24;
	public static final int COMPONENT_SPACE_VERTICAL = 6;
	public static final int COMPONENT_SPACE_HORIZONTAL = 4;
}
